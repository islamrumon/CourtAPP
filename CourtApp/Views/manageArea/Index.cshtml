@model IEnumerable<CourtApp.Models.AREAINF>

@{
    ViewBag.Title = "Index";
}

<h2>List Of Area</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AREANAM)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AREANAM)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AREAID }) |
            
            @Html.ActionLink("Delete", "Delete", new { id=item.AREAID })
        </td>
    </tr>
}

</table>
